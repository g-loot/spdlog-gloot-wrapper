<?xml version="1.0"?>
<package>
    <metadata>
        <id>spdlog-gloot-wrapper</id>
        <version>#VERSION#</version>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Windows x64 spdlog NuGet package</description>
        <tags>spdlog</tags>
        <authors>Joel Sj√∂qvist</authors>
        <repository type="git" url="https://github.com/g-loot/spdlog-gloot-wrapper.git" branch="master"/>
    </metadata>
    <files>
        <file src=".\spdlog\spdlog\*.h" target="spdlog"/>
		<file src=".\spdlog\spdlog\details\*.h" target="spdlog\details"/>
		<file src=".\spdlog\spdlog\fmt\*.h" target="spdlog\fmt"/>
		<file src=".\spdlog\spdlog\sinks\*.h" target="spdlog\sinks"/>
    </files>
</package>
